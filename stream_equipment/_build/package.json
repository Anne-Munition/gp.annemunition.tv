{
  "name": "build",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--no-warnings' vite",
    "build": "yarn clean && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "clean": "rimraf ../!(_build) || exit 0",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --ignore-pattern '__tests__' .",
    "prettier": "prettier --write \"**/*.{js,ts,md,*rc,json}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.6.7",
    "roboto-fontface": "*",
    "vue": "^3.4.18",
    "vuetify": "^3.5.3"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^1.8.27"
  }
}
